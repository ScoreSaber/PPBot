      "discord-api-types": "^0.37.79",
      "discord.js": "^14.14.1",
      "dotenv": "^16.4.5",
      "ffmpeg": "^0.0.4",
      "fluent-ffmpeg": "^2.1.2",
      "fs-extra": "^11.2.0",
      "knex": "^3.1.0",
      "nodemon": "^3.1.0",
      "tsconfig-paths": "^4.2.0",
