      "discord-api-types": "^0.22.0",
      "discord-buttons": "^4.0.0",
      "discord.js": "^12.5.3",
      "dotenv": "^10.0.0",
      "ffmpeg": "^0.0.4",
      "fluent-ffmpeg": "^2.1.2",
      "fs-extra": "^10.0.0",
      "knex": "^0.95.10",
      "nodemon": "^2.0.12",
      "tsconfig-paths": "^3.10.1",
